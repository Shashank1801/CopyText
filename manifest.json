{
  "manifest_version": 2,

  "name": "Copy Text",
  "description": "This extension save text and copy on clipboard when needed.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "storage"
  ]
}
